{
  "name": "auto-type-dep",
  "version": "1.0.0",
  "description": "a command to automatically fill up the @types corresponding to the dependency when you install a dependency",
  "bin": {
    "auto-type-dep": "bin/index.js"
  },
  "scripts": {
    "build": "npx babel ./src --out-dir ./bin"
  },
  "keywords": [
    "npm",
    "yarn",
    "@types",
    "typescript"
  ],
  "author": "rwson",
  "license": "ISC",
  "dependencies": {
    "@kenworks/colorconsole": "^0.2.0",
    "commander": "^5.0.0",
    "cross-spawn": "^7.0.2",
    "ini": "^1.3.5",
    "read-pkg": "^5.2.0",
    "update-notifier": "^4.1.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5"
  }
}
